{
    "version": 2,
    "functions": {
        "api/app.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [{
            "source": "/api/(.*)",
            "destination": "/api/app.js"
        },
        {
            "source": "/(.*)",
            "destination": "/client/dist/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/client/dist/index.html"
        }
    ]
}